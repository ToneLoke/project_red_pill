{
  "name": "project_red_pill_server",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.4.10",
    "morgan": "^1.9.1",
    "socket.io": "^2.2.0",
    "typeface-roboto": "^0.0.54"
  },
  "scripts": {
    "reset:server": "rm -rf node_modules package-lock.json && npm i",
    "reset:client": "npm run reset --prefix client",
    "reset": "npm run reset:server && npm run reset:client",
    "build": "babel api_service -d dist",
    "start": "npm run build && node dist/server.js",
    "start:dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "npm run start --prefix client",
    "dev:server": "nodemon --exec babel-node api_service/server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run reset:client && npm run build --prefix client",
    "debug": "nodemon --inspect --exec babel-node api_service/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "nodemon": "^1.18.9"
  }
}
